<div id="main">

<div id="albums" class="clearfix">
  <% @albums.each do |album| %>
  	<div id="home_album" class="album"><%= link_to("/album/#{album.url}", :remote => true, :rel => 'album') do %>
      <%= image_tag("#{album.album_cover}", width: '340', class: "homepage") %>
    <% end %>
    </div>
  <% end %>
</div>
</div>
<div id="week" class="clearfix"><a href="/"><h4>WEEK 
  <% if @albums.first != nil %>
  <%= @albums.first.week %>: 
  <%= @albums.first.week_name.upcase %>
  <% end %>
  </h4></a></div>
  <div id="previous"><img src="https://s3.amazonaws.com/UnfoundSounds/images/previous.png"></div>
  <div id="next"><img src="https://s3.amazonaws.com/UnfoundSounds/images/next.png"></div>